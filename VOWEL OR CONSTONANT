// VOWELS ND CONSTANT

#include <stdio.h>

int main() {
    char X;

    printf(" ENTER ALPHABET. ");
    scanf("%s", &X);
  
     if (X=='a'||X=='e'||X=='i'||X=='o'||X=='u'||X=='A'||X=='E'||X=='I'||X=='O'||X=='U') 
        printf(" alphabet is VOWEL.");
     
     else 
         printf("alphabet is CONSTONANTS.");
      return 0;
}
/*
ENTER ALPHABET. h
alphabet is CONSTONANTS.

